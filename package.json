{
	"name": "sika-activty-service",
	"version": "0.2.15",
	"description": "RESTful API for interacting with activities. Lives at [https://activity-service.fly.dev](https://activity-service.fly.dev).",
	"engines": {
		"node": ">= 20.8.1"
	},
	"scripts": {
		"start": "node dist/src/index.js",
		"dev": "docker compose config && docker compose run api --abort-on-container-exit",
		"_dev": "nodemon -x ts-node src/index.ts",
		"lint": "npm run lint:ts ; npm run format",
		"lint:ts": "eslint --ignore-path .gitignore --fix .",
		"format": "prettier . --write",
		"build": "tsc",
		"build:docker": "docker compose build",
		"test": "docker compose run test",
		"test:watch": "docker compose run api npm run _test:watch",
		"_test": "vitest --run --no-threads --coverage",
		"_test:watch": "vitest --watch --no-threads"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sikaeducation/activity-service.git"
	},
	"keywords": [],
	"author": "Kyle Coberly",
	"license": "Proprietary",
	"bugs": {
		"url": "https://github.com/sikaeducation/activity-service/issues"
	},
	"homepage": "https://github.com/sikaeducation/activity-service#readme",
	"dependencies": {
		"@feathersjs/adapter-commons": "^5.0.11",
		"@feathersjs/authentication": "^5.0.11",
		"@feathersjs/authentication-client": "^5.0.11",
		"@feathersjs/configuration": "^5.0.11",
		"@feathersjs/errors": "^5.0.11",
		"@feathersjs/express": "^5.0.11",
		"@feathersjs/feathers": "^5.0.11",
		"@feathersjs/mongodb": "^5.0.11",
		"@feathersjs/schema": "^5.0.11",
		"@feathersjs/socketio": "^5.0.11",
		"@feathersjs/transport-commons": "^5.0.11",
		"@feathersjs/typebox": "^5.0.11",
		"adm-zip": "^0.5.9",
		"axios": "^1.1.3",
		"dotenv": "^16.3.1",
		"feathers-swagger": "^3.0.0",
		"jsonwebtoken": "^9.0.2",
		"jwks-rsa": "^3.0.0",
		"lodash": "^4.17.21",
		"mongodb": "^6.1.0",
		"morgan": "^1.10.0",
		"octokit": "^3.1.1",
		"pm2": "^5.3.0",
		"swagger-ui-dist": "^5.9.0",
		"winston": "^3.11.0"
	},
	"devDependencies": {
		"@feathersjs/cli": "^5.0.11",
		"@rushstack/eslint-patch": "^1.5.1",
		"@sikaeducation/eslint-config": "^3.1.7",
		"@types/adm-zip": "^0.5.0",
		"@types/lodash": "^4.14.200",
		"@types/morgan": "^1.9.3",
		"@types/node": "^20.8.1",
		"@types/supertest": "^2.0.12",
		"@types/swagger-ui-dist": "^3.30.3",
		"@vitest/coverage-v8": "^0.34.6",
		"eslint": "^8.22.0",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.3",
		"supertest": "^6.2.4",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.2.2",
		"vite": "^4.5.0",
		"vitest": "^0.34.6"
	},
	"feathers": {
		"language": "ts",
		"packager": "npm",
		"database": "mongodb",
		"framework": "express",
		"transports": [
			"rest",
			"websockets"
		],
		"schema": "typebox"
	},
	"directories": {
		"lib": "src"
	}
}
